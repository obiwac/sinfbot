{
  "compilerOptions": {
    "target": "ESNext",
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "module": "CommonJS",
    "moduleResolution": "Node",
    "esModuleInterop": true,
    "incremental": false,
    "removeComments": true,
    "resolveJsonModule": true,
    "sourceMap": false,
    "outDir": "dist"
  },
  "include": [
    "src"
  ],
  "exclude": [
    "node_modules",
    "dist",
    "llnux"
  ]
}
